{
  "expo": {
    "name": "ResetPulse",
    "slug": "resetPulse",
    "version": "1.1.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F5F0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.irimwebforge.resetpulse",
      "entitlements": {
        "com.apple.developer.in-app-purchases": []
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#4A5568",
          "sounds": [
            "./assets/sounds/bell_short.wav"
          ]
        }
      ]
    ],
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#4A5568",
      "androidMode": "default",
      "androidCollapsedTitle": "ResetPulse"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.irimwebforge.resetpulse",
      "permissions": [
        "android.permission.SCHEDULE_EXACT_ALARM"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f98f80b8-4a15-4ece-afa5-5311174f67b8"
      }
    }
  }
}

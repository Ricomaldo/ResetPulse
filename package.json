{
  "name": "resetpulse",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:critical": "jest --testPathPattern=\\.critical\\.test\\.js$ --no-coverage",
    "test:hooks": "jest src/hooks/__tests__ --no-coverage",
    "test:timer": "jest useTimer --no-coverage",
    "test:dial": "jest useDialOrientation --no-coverage",
    "detect:transforms": "node scripts/detect-transform-needs.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "expo": "~51.0.0",
    "expo-haptics": "~13.0.1",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "^29.0.11",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.2",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-svg": "15.2.0"
  },
  "private": true,
  "devDependencies": {
    "@testing-library/react-native": "^12.9.0",
    "jest": "^29.7.0",
    "jest-expo": "^51.0.4"
  }
}

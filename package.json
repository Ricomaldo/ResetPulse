{
  "name": "resetpulse",
  "version": "1.0.5",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:critical": "jest --testPathPattern=\\.critical\\.test\\.js$ --no-coverage",
    "test:hooks": "jest src/hooks/__tests__ --no-coverage",
    "test:timer": "jest useTimer --no-coverage",
    "test:dial": "jest useDialOrientation --no-coverage",
    "detect:transforms": "node scripts/detect-transform-needs.js",
    "version:patch": "node scripts/version-bump.js patch",
    "version:minor": "node scripts/version-bump.js minor",
    "version:major": "node scripts/version-bump.js major",
    "version:set": "node scripts/version-bump.js set"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@types/jest": "^30.0.0",
    "babel-preset-expo": "~54.0.0",
    "expo": "54.0.12",
    "expo-asset": "^12.0.9",
    "expo-audio": "^1.0.13",
    "expo-haptics": "~15.0.7",
    "expo-notifications": "~0.32.12",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "~29.0.12",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.12",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-purchases": "^9.5.3",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-svg": "15.12.1",
    "react-native-worklets": "0.5.1"
  },
  "private": true,
  "devDependencies": {
    "jest": "^29.7.0"
  }
}
